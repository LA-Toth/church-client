{
  "name": "churchsite-client",
  "version": "0.0.1",
  "description": "Timetable Client",
  "main": "src/index.ts",
  "author": "Laszlo Attila Toth",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && webpack --config webpack.config.prod.js",
    "build:dev": "yarn clean && webpack --config webpack.config.dev.js --bail",
    "clean": "rm -rf dist && rm -rf coverage",
    "eslint": "eslint 'src/**/*.tsx'",
    "eslint:fix": "yarn prettier && yarn eslint --fix",
    "lint": "yarn eslint && yarn typecheck",
    "prettier": "prettier 'src/**/*.tsx' --write",
    "start": "webpack-dev-server --config webpack.config.dev.js --open",
    "test": "yarn test:react-current",
    "test:react-current": "echo \"testing current React version\" && jest --config jest/config.js",
    "test:watch": "yarn test --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@types/enzyme": "^3.10.4",
    "@types/jest": "^24.0.23",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.3.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-typescript": "^0.14.0",
    "file-loader": "^5.0.2",
    "fileloader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "xxhashjs": "^0.2.2",
    "yarn": "^1.21.0"
  },
  "peerDependencies": {}
}
